[
  {
    "id": "sys-1",
    "name": "List Files Detailed",
    "command": "ls -lah",
    "description": "",
    "category": "System"
  },
  {
    "id": "sys-2",
    "name": "Disk Usage",
    "command": "df -h",
    "description": "",
    "category": "System"
  },
  {
    "id": "sys-3",
