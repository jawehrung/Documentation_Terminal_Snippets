    "name": "Kill Process by PID",
    "command": "kill -9 ${VAR:Process ID}",
    "description": "",
    "category": "System"
  },
  {
    "id": "sys-8",
    "name": "Kill Process by Name",
    "command": "pkill ${VAR:Process name}",
    "description": "",
    "category": "System"
  },
  {
    "id": "sys-9",
