    "command": "du -sh ${VAR:Directory path:.}",
    "description": "",
    "category": "System"
  },
  {
    "id": "sys-4",
    "name": "Find Files by Name",
    "command": "find ${VAR:Directory path:.} -name \"${VAR:File pattern}\"",
    "description": "",
    "category": "System"
  },
  {
    "id": "sys-5",
    "name": "Find Large Files",
    "command": "find ${VAR:Directory path:.} -type f -size +${VAR:Size:100M}",
    "description": "",
    "category": "System"
  },
  {
